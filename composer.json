{
  "name": "someonefamous/auditable-values",
  "description": "Auditable Values",
  "keywords": ["laravel", "audit"],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Someone Famous",
      "email": "someonefamous@password99.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "nesbot/carbon": "^2.42",
    "illuminate/support": ">=5"
  },
  "require-dev": {
    "orchestra/testbench": "^6.0",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "SomeoneFamous\\AuditableValues\\": "src",
      "SomeoneFamous\\AuditableValues\\Database\\Factories\\": "database/factories"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SomeoneFamous\\AuditableValues\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-f": "vendor/bin/phpunit --filter"
  },
  "extra": {
    "laravel": {
      "providers": [
        "SomeoneFamous\\AuditableValues\\AuditableValuesServiceProvider"
      ]
    }
  }
}
